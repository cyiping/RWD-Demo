<%@page contentType="text/html;charset=UTF-8"%>
<html>
<head>
<title>EL Arithmetic</title>
</head>

<body>

	<h2>加法:</h2>
	<p>\${1 + 5} => ${1 + 5}</p>

	<h2>減法:</h2>
	<p>\${1 - 5} => ${1 - 5}</p>

	<h2>乘法:</h2>
	<p>\${1 * 5} => ${1 * 5}</p>

	<h2>除法:</h2>
	<p>\${5 / 3} => ${5 / 3}</p>
	<p>\${5 div 3} => ${5 div 3}</p>

	<h2>餘數:</h2>
	<p>\${5 % 2} => ${5 % 2}</p>
	<p>\${5 mod 2} => ${5 mod 2}</p>

</body>
</html>
